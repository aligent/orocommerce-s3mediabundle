{
  "name": "aligent/orocommerce-s3mediabundle",
  "description": "Amazon S3 Based media storage for Oro Platform (OroCommerce and OroCRM)",
  "homepage": "https://github.com/aligent/orocommerce-s3mediabundle",
  "keywords": [
    "Oro",
    "OroPlatform",
    "OroCommerce",
    "OroCRM",
    "Amazon",
    "S3",
    "AWS"
  ],
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Adam Hall",
      "email": "adam.hall@aligent.com.au",
      "homepage": "http://www.aligent.com.au/"
    }
  ],
  "repositories": {
    "oro": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "oro/platform": "6.1.*",
    "gaufrette/aws-s3-adapter": "^0.4.0",
    "knplabs/gaufrette": "^0.11",
    "aws/aws-sdk-php": "^3.17.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.7",
    "phpmd/phpmd": "^2.12",
    "friendsofphp/php-cs-fixer": "~2.18.2 || ~3.1.0",
    "nelmio/alice": "~3.8.0 || ~3.9.0",
    "theofidry/alice-data-fixtures": "~1.4.0 || ~1.5.0",
    "symfony/phpunit-bridge": "~4.4.24 || ~6.1.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Aligent\\S3MediaBundle\\": "./src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  }
}
